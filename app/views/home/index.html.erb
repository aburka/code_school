<h1>All Students</h1>

<table>
 <thead>
  <tr>
   <th scope="col">Student ID</th>
   <th scope="col">Student Name</th>
   <th scope="col">Student Email</th>
  </tr>
 </thead>
 <tbody>
  <% @students.each do |student| %>
    <tr>
     <td><%= student.id %></td>
     <td><%= student.name %></td>
     <td><%= student.email %></td>
    </tr>
  <% end %>
 </tbody>
</table>
